pipeline
{
    agent any 
    stages
    {
        stage('ContinuousDownload-loans')
        {
            steps
            {
               git 'https://github.com/appu0101/Maven1.git'
                   
            }
            
        }
        stage('ContinuousBuild-loans')
        {
            steps
            {
                
              sh 'mvn package'
                  
            }
            
        }
        
    
    }
            
        
        
    
}
