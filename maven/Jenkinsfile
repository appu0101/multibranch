pipeline
{
    agent any 
    stages
    {
        stage('ContinuousDownload')
        {
            steps
            {
                git 'https://github.com/appu0101/declarative.git'
            }
            
        }
        stage('ContinuousBuild')
        {
            steps
            {
                sh 'mvn package'
            }
            
        }

     }
}
